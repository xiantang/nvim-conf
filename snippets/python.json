{
  "dict declaration": {
    "prefix": "dict",
    "body": [
      "${1:key}: ${2:value}"
    ],
    "description": "Snippet for a dictionary"
  },

  "function test": {
    "prefix": "deft",
    "body": [
      "def test_${1:function_name}():",
      "\t$0"
    ],
    "description": "Snippet for test function"
  },

  "if statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition}:",
      "\t$0"
    ],
    "description": "Snippet for if statement"
  },

  "else branch": {
    "prefix": "else",
    "body": [
      "else:",
      "\t$0"
    ],
    "description": "Snippet for else branch"
  },

  "log info": {
    "prefix": "logi",
    "body": [
      "import logging",
      "logging.info('${1:message}')"
    ],
    "description": "log info"
  },

  "log error": {
    "prefix": "loge",
    "body": [
      "import logging",
      "logging.error('${1:message}')"
    ],
    "description": "log error"
  },

  "structure": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName}:",
      "\t$0"
    ],
    "description": "Class structure"
  },

  "slice list": {
    "prefix": "list",
    "body": [
      "${1:list_name} = [${2:elements}]"
    ],
    "description": "List declaration"
  },

  "function with error check": {
    "prefix": "defe",
    "body": [
      "def ${1:function_name}():",
      "\ttry:",
      "\t\t$2",
      "\texcept Exception as e:",
      "\t\tprint('Error:', e)"
    ],
    "description": "Function with error check"
  }
}
